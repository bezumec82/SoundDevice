/dts-v1/;

#include "sama5d2.dtsi"
#include "sama5d2-pinfunc.h"

/ {
    #address-cells = <1>;
    #size-cells = <1>;
    
    model = "Atmel SAMA5D27 Sound Device";
    compatible = "atmel,sama5d2", "atmel,sama5";

    memory {
        reg = <0x20000000 0x08000000>; /* 128MiB */
    }; 
    
    aliases {
        spi0 = &QSPI1;
        serial0=&UART1;
        eth0=&GMAC;
        u-boot,dm-pre-reloc;
    }; /* aliases */

    chosen {
		u-boot,dm-pre-reloc;
        bootargs = "console=ttyS0,115200 earlyprintk root=/dev/mmcblk0p2 rw rootwait";
		stdout-path = "serial0:115200n8";
	}; /* chosen */

    ahb {
        apb { 
            PIO: gpio@fc038000 {
                pinctrl {
                    /* QSPI1 IOSET2 pinout */
                    pinctrl_qspi1_sck_cs_default: qspi1_sck_cs_default {
                        pinmux =    <PIN_PB5__QSPI1_SCK>,
                                    <PIN_PB6__QSPI1_CS>;
                        bias-disable;
                        u-boot,dm-pre-reloc;
                    }; /* pinctrl_qspi1_sck_cs_default */
                    pinctrl_qspi1_dat_default: qspi1_dat_default {
                        pinmux =    <PIN_PB7__QSPI1_IO0>,
                                    <PIN_PB8__QSPI1_IO1>,
                                    <PIN_PB9__QSPI1_IO2>,
                                    <PIN_PB10__QSPI1_IO3>;
                        bias-pull-up;
                        u-boot,dm-pre-reloc;
                    }; /* pinctrl_qspi1_dat_default */
                    
                    /* MAC */
                    pinctrl_macb0_phy_irq: macb0_phy_irq {
                        pinmux =    <PIN_PD31__GPIO>;
                        bias-disable;
                    }; /* macb0_phy_irq */
                    pinctrl_macb0_rmii: macb0_rmii {
                        pinmux =    <PIN_PD9__GTXCK>,
                                    <PIN_PD10__GTXEN>,
                                    <PIN_PD11__GRXDV>,
                                    <PIN_PD12__GRXER>,
                                    <PIN_PD13__GRX0>,
                                    <PIN_PD14__GRX1>,
                                    <PIN_PD15__GTX0>,
                                    <PIN_PD16__GTX1>,
                                    <PIN_PD17__GMDC>,
                                    <PIN_PD18__GMDIO>;
                        bias-disable;
                    }; /* macb0_rmii */

                    /* I2C */
                    pinctrl_i2c0_default: i2c0_default {
                        pinmux =    <PIN_PD21__TWD0>,
                                    <PIN_PD22__TWCK0>;
                        bias-disable;
                    }; /* i2c0 */

                    pinctrl_i2c1_default: i2c1_default {
                        pinmux =    <PIN_PD4__TWD1>,
                                    <PIN_PD5__TWCK1>;
                        bias-disable;
                    }; /* i2c1 */

                    /* UART1 through jLink */
                    pinctrl_uart1_default: uart1_default {
                        pinmux =    <PIN_PD2__URXD1>, /* RX_3U */
                                    <PIN_PD3__UTXD1>; /* TX_3U */
                        bias-disable;
                        u-boot,dm-pre-reloc;
                    }; /* UART1 */

                    /* usb */
                    pinctrl_usb_default: usb_default {
                        pinmux =    <PIN_PB10__GPIO>;
                        bias-disable;
                    }; /* usb */

                    pinctrl_usba_vbus: usba_vbus {
                        pinmux =    <PIN_PD20__GPIO>;
                        bias-disable;
                    }; /* pinctrl_usba_vbus */  
                    
                    /* SD0 */
                    pinctrl_sdmmc0_cmd_dat_default: sdmmc0_cmd_dat_default {
                        pinmux =    <PIN_PA1__SDMMC0_CMD>,
                                    <PIN_PA2__SDMMC0_DAT0>,
                                    <PIN_PA3__SDMMC0_DAT1>,
                                    <PIN_PA4__SDMMC0_DAT2>,
                                    <PIN_PA5__SDMMC0_DAT3>,
                                    <PIN_PA6__SDMMC0_DAT4>,
                                    <PIN_PA7__SDMMC0_DAT5>,
                                    <PIN_PA8__SDMMC0_DAT6>,
                                    <PIN_PA9__SDMMC0_DAT7>;
                        bias-pull-up;
                        u-boot,dm-pre-reloc;
                    }; /* sdmmc0_cmd_dat */
                    pinctrl_sdmmc0_ck_cd_default: sdmmc0_ck_cd_default {
                        pinmux =    <PIN_PA0__SDMMC0_CK>,
                                    // <PIN_PA10__SDMMC0_RSTN>,                                    
                                    <PIN_PA13__SDMMC0_CD>,
                                    <PIN_PA12__SDMMC0_WP>;
                        bias-disable;
                        u-boot,dm-pre-reloc;
                    }; /* sdmmc0_ck_cd */

                    /* SD1 */
                    pinctrl_sdmmc1_cmd_dat_default: sdmmc1_cmd_dat_default {
                        pinmux =    <PIN_PA28__SDMMC1_CMD>,
                                    <PIN_PA18__SDMMC1_DAT0>,
                                    <PIN_PA19__SDMMC1_DAT1>,
                                    <PIN_PA20__SDMMC1_DAT2>,
                                    <PIN_PA21__SDMMC1_DAT3>;
                        bias-pull-up;
                        u-boot,dm-pre-reloc;
                    }; /* sdmmc1_cmd_dat */
                    pinctrl_sdmmc1_ck_cd_default: sdmmc1_ck_cd_default {
                        pinmux =    <PIN_PA22__SDMMC1_CK>,
                                    <PIN_PA30__SDMMC1_CD>;
                        bias-disable;
                        u-boot,dm-pre-reloc;
                    }; /* sdmmc1_ck_cd */
                                        
                }; /* pinctrl */
            }; /* PIO */        
        
            QSPI1: spi@f0024000 {
                pinctrl-names = "default";
                pinctrl-0 = <   &pinctrl_qspi1_sck_cs_default   \
                                &pinctrl_qspi1_dat_default      >;
                status = "okay";
                u-boot,dm-pre-reloc;
                    spi_flash@0 {
                        compatible = "spi-flash";
                        reg = <0>;
                        spi-max-frequency = <66000000>;
                        spi-rx-bus-width = <4>;
                        spi-tx-bus-width = <4>;
                        u-boot,dm-pre-reloc;
                    }; /* spi_flash@0 */
            }; /* QSPI1 */

            GMAC: ethernet@f8008000 {
                pinctrl-names = "default";
                pinctrl-0 = <&pinctrl_macb0_rmii &pinctrl_macb0_phy_irq>;
                phy-mode = "rmii";
                status = "okay";
                    ethernet-phy@1 {
                        reg = <0x1>;
                    }; /* ethernet-phy@1 */
            }; /* GMAC */

            TWIHS0: i2c@f8028000 {
                pinctrl-names = "default";
                pinctrl-0 = <&pinctrl_i2c0_default>;
                status = "okay";
            }; /* TWIHS0 */

            TWIHS1: i2c@fc028000 {
                pinctrl-names = "default";
                pinctrl-0 = <&pinctrl_i2c1_default>;
                status = "okay";
            }; /* TWIHS1 */
            
            UART1: serial@f8020000 {
                pinctrl-names = "default";
                pinctrl-0 = <&pinctrl_uart1_default>;
                status = "okay";
                u-boot,dm-pre-reloc;
            }; /* UART1 */
            
        }; /* apb */
        
        usb1: ohci@00400000 {
            num-ports = <3>;
            atmel,vbus-gpio = <&pioA 42 0>;
            pinctrl-names = "default";
            pinctrl-0 = <&pinctrl_usb_default>;
            status = "okay";
        }; /* usb1 */

        usb2: ehci@00500000 {
            status = "okay";
        }; /* usb2 */

        sdmmc0: sdio-host@a0000000 {
            bus-width = <8>;
            pinctrl-names = "default";
            pinctrl-0 = <   &pinctrl_sdmmc0_cmd_dat_default \
                            &pinctrl_sdmmc0_ck_cd_default>;
            status = "okay";
            u-boot,dm-pre-reloc;
        }; /* sdmmc0 */

        sdmmc1: sdio-host@b0000000 {
            bus-width = <4>;
            pinctrl-names = "default";
            pinctrl-0 = <   &pinctrl_sdmmc1_cmd_dat_default \
                            &pinctrl_sdmmc1_ck_cd_default>;
            status = "okay"; /* conflict with qspi0 */
            u-boot,dm-pre-reloc;
        }; /* sdmmc1 */
                
    }; /* ahb */  
};
