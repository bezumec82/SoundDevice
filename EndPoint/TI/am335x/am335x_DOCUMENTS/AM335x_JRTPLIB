export TI_PTH=/media/constantine/Work/SoundDevice/SoundDevice_endPoint/TI
export AM335_PTH=${TI_PTH}/am335x
export TCH_PTH=${TI_PTH}/gcc-linaro-7.2.1-2017.11-x86_64_arm-linux-gnueabihf/bin
export CC=${TCH_PTH}/arm-linux-gnueabihf-
export PATH=${TCH_PTH}:${PATH}
export ARCH=arm
export CROSS_COMPILE=arm-linux-gnueabihf-
export JRTPLIB_PTH=${AM335_PTH}/jrtplib-3.11.1

reset
stat ${TI_PTH}
stat ${AM335_PTH}
stat ${TCH_PTH}
stat ${JRTPLIB_PTH}


cd ${JRTPLIB_PTH}
mkdir ${JRTPLIB_PTH}/install

export CMAKE_SYSTEM_NAME=Linux
export CMAKE_SYSTEM_PROCESSOR=arm
export CMAKE_C_COMPILER=${TCH_PTH}/arm-linux-gnueabihf-gcc
export CMAKE_CXX_COMPILER=${TCH_PTH}/arm-linux-gnueabihf-g++
export CMAKE_INSTALL_PREFIX=${JRTPLIB_PTH}/install

export CMAKE_FIND_ROOT_PATH_MODE_PROGRAM=NEVER
export CMAKE_FIND_ROOT_PATH_MODE_LIBRARY=ONLY
export CMAKE_FIND_ROOT_PATH_MODE_INCLUDE=ONLY
export CMAKE_FIND_ROOT_PATH_MODE_PACKAGE=ONLY

rm -rf ${JRTPLIB_PTH}/CMakeFiles
rm -rf ${JRTPLIB_PTH}/CMakeCache.txt
rm -rf ${JRTPLIB_PTH}/cmake_install.cmake
rm -rf ${JRTPLIB_PTH}/cmake/JRTPLIBConfig.cmake
rm -rf ${JRTPLIB_PTH}/Makefile
rm -rf ${JRTPLIB_PTH}/src/CMakeFiles
rm -rf ${JRTPLIB_PTH}/src/cmake_install.cmake
rm -rf ${JRTPLIB_PTH}/src/Makefile
rm -rf ${JRTPLIB_PTH}/pkgconfig/jrtplib.pc
rm -rf ${JRTPLIB_PTH}/examples/CMakeFiles
rm -rf ${JRTPLIB_PTH}/examples/cmake_install.cmake
rm -rf ${JRTPLIB_PTH}/examples/Makefile
rm -rf ${JRTPLIB_PTH}/src/rtpconfig.h
rm -rf ${JRTPLIB_PTH}/src/rtplibraryversioninternal.h
rm -rf ${JRTPLIB_PTH}/src/rtptypes.h
reset
cmake . \
  -DCMAKE_SYSTEM_NAME=Linux \
  -DCMAKE_SYSTEM_PROCESSOR=arm \
  -DCMAKE_C_COMPILER=${TCH_PTH}/arm-linux-gnueabihf-gcc \
  -DCMAKE_CXX_COMPILER=${TCH_PTH}/arm-linux-gnueabihf-g++ \
  -DCMAKE_INSTALL_PREFIX=${JRTPLIB_PTH}/install \
  -DCMAKE_SYSROOT=${AM335_PTH}/IDE_SUPPORT/sysroots/armv7ahf-neon-poky-linux-gnueabi

Eclipse
JRTPLIB_PTH
${AM335_PTH}/jrtplib-3.11.1
${JRTPLIB_PTH}/install/lib
${JRTPLIB_PTH}/install/include

LD_FLAGS
-t --verbose --sysroot=${SDKTARGETSYSROOT} -l:libjrtp.a
