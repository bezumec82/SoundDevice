24.576MHz
GPIO1_27        V17     EN
GPIO1 4804c000  gpiochip32
echo $((27+32)) >       /sys/class/gpio/export
echo out >              /sys/class/gpio/gpio$((27+32))/direction
echo 1 >                /sys/class/gpio/gpio$((27+32))/value










